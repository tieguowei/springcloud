<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans        
    http://www.springframework.org/schema/beans/spring-beans.xsd        
    http://code.alibabatech.com/schema/dubbo        
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="app-consumer" />


	<!-- 开发环境需要打开，本地zookeeper -->
	<dubbo:registry address="${dubbo.zookeeper}" />
	<dubbo:consumer timeout="60000" check="false" />


	

	<!-- demo 实例 -->
	<dubbo:reference id="lenderService" interface="com.huizhongcf.dubbo.cloud.api.service.LenderService" />

</beans>