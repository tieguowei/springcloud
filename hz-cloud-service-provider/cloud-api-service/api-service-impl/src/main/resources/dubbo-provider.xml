<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans        
    http://www.springframework.org/schema/beans/spring-beans.xsd        
    http://code.alibabatech.com/schema/dubbo        
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="cloud-api-provider" />
	
	
	<!-- 开发环境需要打开-->
	<dubbo:registry address="${dubbo.zookeeper}"/>
	<dubbo:protocol name="dubbo" port="20990" accesslog="true" />
	<dubbo:provider timeout="60000" delay="-1" />








	
	
	<!-- demo -->
	<bean id="lenderService" class="com.huizhongcf.dubbo.cloud.service.api.impl.LenderServiceImpl" />
	<dubbo:service interface="com.huizhongcf.dubbo.cloud.api.service.LenderService" ref="lenderService" />
	
	
	
	
</beans>
